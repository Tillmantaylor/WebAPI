@{
    ViewData["Title"] = "User Page";
}
<h1>@ViewData["Title"]</h1>

<html>
<head>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-1BmE4kWBq78iYhFldvKuhfTAU6auU8tT94WrHftjDbrCEXSU1oBoqyl2QvZ6jIW3" crossorigin="anonymous">
</head>
<body>
    <p>Use this page to detail your site's user actions.</p>
    <div class="card" style="width: 18rem;">
        <div class="card-body">
            <h5 class="card-title">Pet Store Project</h5>
            <h6 class="card-subtitle mb-2 text-muted">Card subtitle</h6>
            <p class="card-text">A project that manages a Pet Store.</p>
            <a asp-controller="User" asp-action="MetricSelect" class="btn btn-primary">Go to project metric select page</a>
        </div>
    </div>
</body>
</html>